{
  "expo": {
    "name": "Köy Derneği",
    "slug": "dernek-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dernek.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1e40af"
      },
      "package": "com.dernek.mobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": ["expo-router"],
    "scheme": "dernek",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-eas-project-id"
      }
    }
  }
}
